from datetime import datetime
a = input("Nháº­p ngÃ y sinh (dd/mm/yyyy): ")
b = datetime.strptime(a, "%d/%m/%Y")
c = datetime.today()
age = c.year - b.year
# Kiá»ƒm tra Ä‘Ã£ qua sinh nháº­t nÄƒm nay chÆ°a
d = b.replace(year=c.year)
if c < d:
    age -= 1
    print("Báº¡n chÆ°a Ä‘áº¿n sinh nháº­t nÄƒm nay")
elif c == d:
    print("HÃ´m nay lÃ  sinh nháº­t cá»§a báº¡n! ğŸ‰ğŸ¥³")
else:
    print("Báº¡n Ä‘Ã£ qua sinh nháº­t nÄƒm nay.")
print("Tuá»•i cá»§a báº¡n:", age)