n = int(input("Nhap so ban muon kiem tra:"))
if n<=1:
  print("Day khong phai so nguyen to")
else:
  nguyen_to = True
  for i in range(2,int(n**0.5)+1):
    if n%i == 0:
      nguyen_to = False
      break
if nguyen_to == True:
  print("Day la so nguyen to")
else:
  print("Day khong phai la so nguyen to")
